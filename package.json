{
  "name": "code-coverage-reporter",
  "version": "1.1.2",
  "description": "A GitHub Action to report code coverage within Pull Requests",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "cross-env INPUT_COVERAGE_THRESHOLD=80 INPUT_INCLUDE_PATTERN='tmp_test_files/.*file.*$' INPUT_EXCLUDE_PATTERN='\\.excluded' node --test '**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fermi-ad/code-coverage-reporter.git"
  },
  "author": "Fermi National Accelerator Laboratory - Accelerator Directorate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fermi-ad/code-coverage-reporter/issues"
  },
  "homepage": "https://github.com/fermi-ad/code-coverage-reporter#readme",
  "dependencies": {
    "@actions/core": "^2.0.3",
    "@actions/github": "^9.0.0"
  }
}
